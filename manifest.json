{
  "manifest_version": 2,
  "name": "Pass",
  "background": {
    "scripts": ["lib/eventPage.js"]
  },
  "description": "Effortlessly save and share across devices text, pictures, music and more!",
  "key": "",
  "version": "0.2",
  "icons":{
	"48":"icon48.png",
	"128":"icon128.png"
	},
	"author":"Luan Ngo",
	
  "browser_action": {
    "default_icon": "icon36.png",
    "default_popup": "index.html"
  },
  "permissions": [
	"notifications",
	"clipboardWrite",
    "http://www.local-motion.ca/",
	    "https://accounts.google.com/o/oauth2/token",
    "https://www.googleapis.com/chromewebstore/v1.1/notifications",
	"tabs",
	"gcm", 
	"storage"
  ]
}
